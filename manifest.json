{
	"manifest_version": 2,
	"name": "Time Tracker",
	"version": "0.1",
	"description": "Keeps track of how much time you spend on various web sites.",

	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"permissions": [
		"tabs"
	],

	"browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
	},

	"icons": { "128": "images/icon.png" }
}
